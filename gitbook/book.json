{
    "title" : "EXP-Blog",
    "author" : "EXP",
    "description" : "我的博客",
    "language" : "en",
    "links" : {
        "sidebar" : {
            "Github" : "https://www.baidu.com"
        }
    },
    "plugins": [
        "code",
        "prism",
        "-back-to-top-button",
        "anchor-navigation-ex",
        "splitter",
        "search-pro",
        "-page-copyright",
        "tbfed-pagefooter",
        "katex",
        "donate",
        "simple-page-toc",
        "-sharing",
        "sharing-plus",
        "todo",
        "auto-scroll-table",
        "image-captions",
        "toggle-chapters",
        "multipart",
        "pageview-count",
        "theme-comscore",
        "-theme-api",
        "-theme-faq",
        "edit-link",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "code": {
            "copyButtons": true
        },
        "page-copyright": {
          "description": "modified at",
          "signature": "EXP",
          "wisdom": "Designer, Frontend Developer & overall web enthusiast",
          "format": "YYYY-MM-dd hh:mm:ss",
          "copyright": "Copyright &#169; EXP",
          "timeColor": "#666",
          "copyrightColor": "#666",
          "utcOffset": "8",
          "style": "normal",
          "noPowered": false
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy EXP 2019",
            "modify_label": "本书发布时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "donate": {
            "wechat": "微信收款的二维码URL",
            "alipay": "支付宝收款的二维码URL",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "weibo": true,
            "qq": true,
            "all": [
                "douban", "google", "instapaper", "linkedin", "twitter", "messenger", "qzone", "viber", "whatsapp"
            ]
        },
        "image-captions": {
          "caption": "Image _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
        },
        "theme-default": {
            "showLevel": false
        },
        "theme-api": {
            "theme": "dark"
        },
        "edit-link": {
            "base": "https://github.com/USER/REPO/edit/BRANCH",
            "label": "Edit This Page"
        },
        "sitemap-general": {
            "prefix": "http://exp.gitbook.com"
        }
    }
}

